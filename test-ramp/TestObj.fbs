namespace my.test;

attribute "priority";

enum Gender:byte (bit_flags) { Male = 0, Female = 1, }

table Person(native_custom_alloc:"SAllocator") {
  name:string;
  gender:Gender;
  age:int;
}

table Class(native_custom_alloc:"SAllocator") {
  name:string;
  students:[Person];
  teacher:Person;
  scores:[int];
}

root_type Class;
